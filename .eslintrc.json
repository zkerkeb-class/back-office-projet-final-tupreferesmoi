{
    "env": {
      "browser": true,
      "es5": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 5,
      "sourceType": "module"
    },
    "plugins": ["react", "jsx-a11y", "import"],
    "rules": {
      "react/prop-types": "off",
      "no-console": "warn",
      "strict": ["error", "global"]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }